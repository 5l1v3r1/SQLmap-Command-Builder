    <script>
        function ChangeColor(clo) {

            document.getElementById("content_con").style.backgroundColor = clo;
            document.getElementById("div_bg").style.backgroundColor = clo;
            var h = document.getElementById("content_con").innerHTML;
            h = h.replace(/background-color:#F9F6AF/g, "background-color:" + clo);
            h = h.replace(/background-color:#9DDBF4/g, "background-color:" + clo);
            h = h.replace(/background-color:#DEDEDE/g, "background-color:" + clo);
            h = h.replace(/background-color:#F9D1D9/g, "background-color:" + clo);
            h = h.replace(/background-color:#E4C8F8/g, "background-color:" + clo);
            h = h.replace(/background-color:#FFFFFF/g, "background-color:" + clo);

            document.getElementById("content_con").innerHTML = h;

        }
        function share() {
            if (document.getElementById("bshare").style.display == "block")
                document.getElementById("bshare").style.display = "none";
            else
                document.getElementById("bshare").style.display = "block";
        }

        function changeFontSize(s) {

            document.getElementById("content_con").style.fontSize = s + "px";
            $("#content_con").find("p").css("font-size", s + "px");
            $("#content_con p").each(function (index) {
                $(this).css("font-size", s + "px");


            })


        }
    </script>
